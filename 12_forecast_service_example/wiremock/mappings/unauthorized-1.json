{
  "priority": 10,
  "request": {
    "method": "ANY",
    "urlPattern": ".*",
    "headers": {
      "X-Api-Key": {
        "absent": true
      }
    }
  },
  "response": {
    "status": 401,
    "jsonBody": {
      "error": "Missing API key"
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
